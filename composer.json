{
    "name": "jumper423/voximplant-phpsdk",
    "description": "Voximplant HTTP API php sdk",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://voximplant.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Ivaneychik",
            "homepage": "https://github.com/DmitryIvaneychik"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Voximplant\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Voximplant\\" : "test/" }
    }
}
